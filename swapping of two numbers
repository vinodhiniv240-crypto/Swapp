class swapp
{
    void swapping(int a,int b)
    {
        System.out.println("before swapping a="+a+",b="+b);
        a=a^b;
        b=a^b;
        a=a^b;
        System.out.println("after swapping a="+a +",b="+b);
    }
    public static void main(String[] args)
    {
        swapp obj=new swapp();
        obj.swapping(10,20);
    }
}
